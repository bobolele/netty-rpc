<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:rpc="http://www.lybo.io/netty-rpc" xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context.xsd
    http://www.lybo.io/netty-rpc
    http://www.lybo.io/netty-rpc/netty-rpc.xsd">
    <rpc:reference id="userService" interfaceName="io.lybo.rpc.test.UserService" />

    <rpc:service interfaceName="" ref="userService" />

    <bean id="userService" class="io.lybo.rpc.test.impl.UserServiceImpl" />
</beans>
